{
    "name": "cardboardmachete/machete",
    "type": "library",
    "description": "A PBBG library",
    "homepage": "https://github.com/cardboardmachete/machete",
    "license": "MIT",
    "authors": [
        {
            "name": "Caleb",
            "email": "caleb@cardboardmachete.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37",
        "illuminate/support": "^8.48"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "CardboardMachete\\Machete\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CardboardMachete\\Machete\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
